{
    "model": "MobileNetV3Small",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 80,

    "optimizer": {
        "type": "adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [40, 65],
            "values": [1e-4, 1e-5, 1e-6]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds",

    "compression":
    {
    "algorithm": "rb_sparsity",
        "sparsity_init": 0.01,
        "params": {
            "sparsity_target": 0.52,
            "sparsity_target_epoch": 35,
            "sparsity_freeze_epoch": 45
        }
    }
}
