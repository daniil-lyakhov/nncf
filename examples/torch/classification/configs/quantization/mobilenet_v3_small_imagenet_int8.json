{
    "model": "mobilenet_v3_small",
    "pretrained": true,
    "input_info": {
      "sample_size": [1, 3, 224, 224]
    },
    "batch_size": 256,
    "epochs": 5,
    "optimizer": {
        "type": "Adam",
        "base_lr": 1e-4,
        "schedule_type": "multistep",
        "steps": [
            3
        ],
        "weight_decay": 1e-05
    },
    "compression": {
        "algorithm": "quantization",
        "preset": "mixed",
        "initializer": {
            "range": {
                "num_init_samples": 300
            },
           "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 0
           }
        },
        "weights": {
                "bits": 8,
                "per_channel": true
        },
        "activations": {
                "bits": 8,
                "per_channel": false
        }
       }
}
