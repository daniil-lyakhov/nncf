{
    "model": "MobileNetV2",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },

    "batch_size": 256,
    "epochs": 9,

    "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [3, 6],
            "values": [1e-3, 1e-4, 1e-5]
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfds",
    
    "compression": {
        "algorithm": "quantization",
        "initializer": {
             "range": {
                 "num_init_samples": 0
             }
         }
    }
}
