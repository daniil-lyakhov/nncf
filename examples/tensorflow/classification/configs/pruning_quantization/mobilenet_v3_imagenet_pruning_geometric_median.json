{
    "model": "MobileNetV3Small",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },
    "batch_size": 320,
    "epochs": 70,
    "optimizer": {
        "type": "sgd",
        "schedule_type": "multistep",
        "schedule_params": {
            "base_lr": 0.1,
            "steps": [20, 40, 60],
            "gamma": 0.1
        },
        "optimizer_params": {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "dataset": "imagenet2012",
    "dataset_type": "tfds",

    "compression": [
        {
            "algorithm": "filter_pruning",
            "pruning_init": 0.01,
            "params": {
                "prune_downsample_convs": true,
                "schedule": "exponential",
                "pruning_target": 0.3,
                "pruning_steps": 30,
                "filter_importance": "geometric_median"
            }
        },
        {
            "algorithm": "quantization",
            "initializer": {
                    "range": {
                        "num_init_samples": 15000
                    },
                    "batchnorm_adaptation": {
                        "num_bn_adaptation_samples": 4000
                    }
            },
            "weights": {
                "per_channel": false
            }
        }]
}
