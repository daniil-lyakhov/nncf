{
    "model": "MobileNetV3Small",
    "pretrained": false,
    "input_info": {
        "sample_size": [1, 32, 32, 3]
    },
    "batch_size": 320,
    "epochs": 200,
    "optimizer": {
        "type": "sgd",
        "schedule_type": "multistep",
        "schedule_params": {
            "base_lr": 0.1,
            "steps": [50, 100, 150],
            "gamma": 0.1
        },
        "optimizer_params": {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "dataset": "cifar100",
    "dataset_type": "tfds",

    "compression": {
        "algorithm": "filter_pruning",
        "pruning_init": 0.1,
        "params": {

            "prune_downsample_convs": true,
            "schedule": "exponential",
            "pruning_target": 0.2,
            "pruning_steps": 10,
            "filter_importance": "geometric_median"
        }
    }
}
